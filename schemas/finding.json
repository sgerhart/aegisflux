{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Finding",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "host_id": {
      "type": "string"
    },
    "severity": {
      "type": "string",
      "enum": [
        "info",
        "low",
        "medium",
        "high",
        "critical"
      ]
    },
    "type": {
      "type": "string"
    },
    "evidence": {
      "type": "object"
    },
    "ts": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": [
    "id",
    "host_id",
    "severity",
    "type",
    "ts"
  ]
}